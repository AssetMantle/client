GET         /                          controllers.IndexController.index

GET         /login                     controllers.LoginController.loginForm
POST        /login                     controllers.LoginController.login

GET         /signUp                    controllers.SignUpController.signUpForm
POST        /signUp                    controllers.SignUpController.signUp

GET         /updateContact             controllers.UpdateContactController.updateContactForm
POST        /updateContact             controllers.UpdateContactController.updateContact

GET         /verifyMobileNumber        controllers.VerifyMobileNumberController.verifyMobileNumberForm
POST        /verifyMobileNumber        controllers.VerifyMobileNumberController.verifyMobileNumber

GET         /verifyEmailAddress        controllers.VerifyEmailAddressController.verifyEmailAddressForm
POST        /verifyEmailAddress        controllers.VerifyEmailAddressController.verifyEmailAddress

GET         /assets/*file              controllers.Assets.versioned(path="/public", file: Asset)

