GET         /                          controllers.IndexController.index

POST        /login                     controllers.LoginController.login

POST        /signUp                    controllers.SignUpController.signUp

POST        /updateContact             controllers.UpdateContactController.updateContact

POST        /verifyMobileNumber        controllers.VerifyMobileNumberController.verifyMobileNumber

POST        /verifyEmailAddress        controllers.VerifyEmailAddressController.verifyEmailAddress

GET         /assets/*file              controllers.Assets.versioned(path="/public", file: Asset)

GET         /message                   controllers.AsyncController.message

GET         /find                      controllers.AsyncController.find

GET         /delete                    controllers.AsyncController.delete

GET         /addowner                  controllers.AsyncController.addOwner

GET         /addfiat                   controllers.AsyncController.addFiat

GET         /delowner                  controllers.AsyncController.delOwner

